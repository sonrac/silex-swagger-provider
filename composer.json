{
    "name": "sonrac/silex-swagger-provider",
    "description": "Silex swagger provider with ui",
    "type": "library",
    "require": {
        "swagger-api/swagger-ui": "~3.0",
        "symfony/console": "~2.8|~3.0",
        "silex/silex": "~2.0",
        "zircote/swagger-php": "3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "symfony/browser-kit": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "sonrac\\Swagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sonrac\\Swagger\\Tests\\": "tests/",
            "sonrac\\Swagger\\Tests\\Functional\\": "tests/functional/"
        }
    },
    "suggest": {
        "doctrine/migrations": ">=1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Donii Sergii",
            "email": "s.donii@infomir.com"
        }
    ],
    "minimum-stability": "stable"
}
